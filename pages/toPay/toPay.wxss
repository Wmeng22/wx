#remindUsers{
    width: 592rpx;
    height: 716rpx;
    background-color: #0e0f0f;
    position: fixed;
    left: 50%;
    margin-left: -286rpx;
    margin-top: 200rpx;
    z-index: 999;
    box-sizing: border-box;
}
#remindUsers .title{
    color: #c6451b;
    font-size: 60rpx;
    font-weight: bold;
    text-align: center;
    margin: 36rpx 0;
}
#remindUsers .content{
    font-size: 30rpx;
    text-align: center;
    color: #575757;
    height: 34rpx;
    line-height: 34rpx;
}
#remindUsers .content.last{
    line-height: 98rpx;
    height: 98rpx;
}
#remindUsers .toSubmit{
    position: absolute;
    bottom: 0;
    color: #fff;
    text-align: center;
    height: 88rpx;
    line-height: 88rpx;
    font-size: 30rpx;
    background-color: #c6451b;
    width: 100%;
}
#remindUsers .stepImg{
    margin-top: 20rpx;
    margin-left: 30rpx;
    width: 535rpx;
    height: 157rpx;
}
