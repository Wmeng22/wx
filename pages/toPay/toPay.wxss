.list_title {
    margin-top: 44rpx;
    color: #c6451b;
    font-size: 24rpx;
    height: 46rpx;
    line-height: 46rpx;
}

.list_content {
    border-bottom: 1px solid #313131;
    /*height: 79rpx;*/
    line-height: 80rpx;
    font-size: 24rpx;
    color: #818181;
}
.container {
    padding: 0 76rpx;
    padding-bottom: 200rpx
}

.order_status {
    width: 100%;
    color: #c6451b;
    font-size: 24rpx;
    text-align: center
}

.order_img {
    height: 380rpx;
    width: 100%;
    position: relative;
    overflow: hidden
}

.order_img .img, .order_img .kezi {
    width: 675rpx;
    height: 576rpx;
    position: absolute;
    top: -100rpx;
    left: 30rpx;
}

.order_img .kezi .kezi_content {
    position: absolute;
    left: 45rpx;
    top: 310rpx;
    transform: rotate(7deg);
    text-align: center;
    width: 110rpx;
}

.w100 {
    width: 100%;
}

.list_content .pro_name {
    width: 500rpx;
    font-size: 26rpx;
    color: #818181
}

.list_content .pro_name_2 {
    float: left;
    width: 198rpx;
}

.list_content .pro_value_wrap {
    float: left;
    width: 400rpx;
}

.list_content .pro_value {
    line-height: 38 rpx;
    padding: 10 rpx;
    padding-top: 24 rpx;
    width: 100%;
    text-align: right;
}

.order_num {
    height: 79rpx;
    line-height: 80rpx;
    border-bottom: 1rpx solid #313131;
}

.order_num .num {
    color: #c6451b
}

.order_num .price {
    font-size: 40rpx;
    color: #c6451b
}

.promoCode {
    font-size: 28rpx;
    border-bottom: 1px solid #313131;
    height: 79rpx;
    line-height: 80rpx;
}

.codeIconWrap{
    position: absolute;
    right: 0rpx;
    z-index: 99;
    bottom: 0rpx;
    /*background-color: pink;*/
    width: 60rpx;
    height: 60rpx;
    padding-left: 25rpx;
    padding-top: 25rpx;
    /*box-sizing: border-box;*/
}
.codeIcon {
    width: 33rpx;
    height: 33rpx;
}

.order_detail .tip_img {
    position: absolute;
    left: 240rpx;
    bottom: 18rpx
}
.list_style {
    border-bottom: 1px solid #313131;
    padding: 14rpx 0;
}
.list_detail {
    font-size: 26rpx;
    line-height: 52rpx;
}
.list_detail .pro_title {
    color: #818181;
}
.list_detail .pro_title.titleHide {
    color: #1a1a1a
}
.list_detail .pro_des {
    color: #fff;
}
.list_detail .pro_price {
    color: #fff;
    text-align: right;
    float: right;
}
.bottom_new {
    position: fixed;
    bottom: 0;
    height: 88rpx;
    left: 0;
    width: 100%;
    text-align: center
}
.bottom_new .goNext {
    float: left;
    width: 70%;
    line-height: 88rpx;
    background-color: #c6451b;
    color: #fff;
    font-size: 32rpx
}
.bottom_new .exit {
    width: 30%;
    float: left;
    line-height: 88rpx;
    background-color: #1a1a1a;
    color: #c6451b;
    font-size: 32rpx
}
